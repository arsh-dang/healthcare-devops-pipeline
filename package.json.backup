{
  "name": "app1",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@reduxjs/toolkit": "^2.7.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "lodash": "^4.17.21",
    "luxon": "^3.6.1",
    "moment": "^2.30.1",
    "mongoose": "^8.14.0",
    "prom-client": "^14.2.0",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "6.26.1",
    "react-scripts": "5.0.1",
    "reactjs": "^1.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "PORT=3000 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "CI=true react-scripts test --coverage --watchAll=false --reporters=default --reporters=jest-junit",
    "test:backend": "jest server/test --coverage --reporters=default --reporters=jest-junit",
    "test:integration": "node test-integration.js",
    "test:e2e": "cd server && npm run test:e2e",
    "test:performance": "cd server && npm run test:performance",
    "eject": "react-scripts eject",
    "server": "node server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run start\"",
    "lint": "eslint src server --ext .js,.jsx",
    "lint:ci": "eslint src server --ext .js,.jsx --format junit --output-file eslint-report.xml",
    "lint:fix": "eslint src server --ext .js,.jsx --fix",
    "security:scan": "./scripts/advanced-security-scan.sh",
    "docker:build": "docker build -f Dockerfile.frontend -t healthcare-app-frontend . && docker build -f Dockerfile.backend -t healthcare-app-backend .",
    "docker:test": "docker-compose -f docker-compose.test.yml up --abort-on-container-exit",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-router-dom": "^5.3.3",
    "autocannon": "^7.12.0",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.37.5",
    "identity-obj-proxy": "^3.0.0",
    "jest-junit": "^16.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "newman": "^6.0.0",
    "newman-reporter-htmlextra": "^1.22.11",
    "pnpm": "^10.8.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!**/node_modules/**",
      "!**/build/**"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html",
      "cobertura"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 58,
        "statements": 60
      }
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/*.{test,spec}.{js,jsx}"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(autocannon|hdr-histogram-js|@assemblyscript/loader|date-fns|react-router|react-router-dom)/)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "jest-junit": {
    "outputDirectory": ".",
    "outputName": "test-report.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "jest": {
    "projects": [
      {
        "displayName": "frontend",
        "testMatch": ["<rootDir>/src/**/__tests__/**/*.{js,jsx}", "<rootDir>/src/**/*.{test,spec}.{js,jsx}"],
        "collectCoverageFrom": [
          "src/**/*.{js,jsx}",
          "!src/index.js",
          "!src/reportWebVitals.js"
        ],
        "coverageReporters": ["text", "lcov", "html", "cobertura"],
        "transformIgnorePatterns": [
          "node_modules/(?!(autocannon|hdr-histogram-js|@assemblyscript/loader|date-fns|react-router|react-router-dom)/)"
        ],
        "moduleNameMapper": {
          "\\.(css|less|scss|sass)$": "identity-obj-proxy"
        },
        "testEnvironment": "jsdom"
      },
      {
        "displayName": "backend",
        "testMatch": ["<rootDir>/server/test/**/*.{js,jsx}"],
        "collectCoverageFrom": [
          "server/**/*.{js,jsx}",
          "!server/node_modules/**"
        ],
        "coverageReporters": ["text", "lcov", "html", "cobertura"],
        "testEnvironment": "node"
      }
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "server/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!**/node_modules/**",
      "!**/build/**"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html",
      "cobertura"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 58,
        "statements": 60
      }
    }
