{
  "name": "app1",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@reduxjs/toolkit": "^2.9.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "lodash": "^4.17.21",
    "luxon": "^3.7.2",
    "moment": "^2.30.1",
    "mongoose": "^8.18.1",
    "prom-client": "^14.2.0",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "6.26.1",
    "react-scripts": "5.1.0-next.26",
    "reactjs": "^1.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "CI=true react-scripts test --coverage --watchAll=false --testResultsProcessor=jest-junit",
    "test:integration": "node test-integration.js",
    "test:e2e": "cd server && npm run test:e2e",
    "test:performance": "cd server && npm run test:performance",
    "eject": "react-scripts eject",
    "server": "node server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run start\"",
    "lint": "eslint src server --ext .js,.jsx",
    "lint:ci": "eslint src server --ext .js,.jsx --format junit --output-file eslint-report.xml",
    "lint:fix": "eslint src server --ext .js,.jsx --fix",
    "security:scan": "./scripts/advanced-security-scan.sh",
    "docker:build": "docker build -f Dockerfile.frontend -t healthcare-app-frontend . && docker build -f Dockerfile.backend -t healthcare-app-backend .",
    "docker:test": "docker-compose -f docker-compose.test.yml up --abort-on-container-exit",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js"
  },
  "proxy": "http://localhost:5001",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.18.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "autocannon": "^7.15.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.37.5",
    "identity-obj-proxy": "^3.0.0",
    "jest-junit": "^16.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "newman": "^6.2.1",
    "newman-reporter-htmlextra": "^1.23.1",
    "pnpm": "^10.15.1",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!**/node_modules/**",
      "!**/build/**"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html",
      "cobertura"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/*.{test,spec}.{js,jsx}"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(autocannon|hdr-histogram-js|@assemblyscript/loader|date-fns|react-router|react-router-dom)/)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "jest-junit": {
    "outputDirectory": ".",
    "outputName": "test-report.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "jose": "^4.15.5",
    "webpack-dev-server": "^4.15.2",
    "form-data": "^4.0.4",
    "brace-expansion": "^2.0.2",
    "on-headers": "^1.1.0"
  },
  "pnpm": {
    "overrides": {
      "nth-check": "^2.1.1",
      "postcss": "^8.4.31",
      "jose": "^4.15.5",
      "webpack-dev-server": "^4.15.2",
      "form-data": "^4.0.4",
      "brace-expansion": "^2.0.2",
      "on-headers": "^1.1.0"
    }
  }
}
